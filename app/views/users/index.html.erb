<h3 class='center'>User Directory</h3>

<table class='table table-striped table-condensed'>
	<tr>
		<th>User ID</th>
		<th>Email</th>
		<th>Full Name</th>
	</tr>
	<% @users.each do |user| %>
		<tr>
			<td><%= user.id %></td>
			<td><%= user.email %></td>
			<td><%= user.full_name %></td>
		</tr>
	<% end %>
</table>
